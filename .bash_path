# Homebrew - sbin
export PATH="/usr/local/sbin:$PATH"

# Homebrew - coreutils
# export PATH="$(brew --prefix coreutils)/libexec/gnubin:$PATH"

# Haskell - GHCup
[ -f "/Users/wen/.ghcup/env" ] && source "/Users/wen/.ghcup/env" # ghcup-env

# Rust - Cargo
. "$HOME/.cargo/env"

# Python - PyEnv
export PYENV_ROOT="$HOME/.pyenv"
command -v pyenv >/dev/null || export PATH="$PYENV_ROOT/bin:$PATH"
eval "$(pyenv init -)"

# Agda
export PATH="$HOME/.agdaup/bin:$PATH"
export Agda_datadir="$HOME/.agdaup/data"


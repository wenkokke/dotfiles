# Homebrew: sbin
export PATH="/usr/local/sbin:$PATH"

# Homebrew: Ruby
export PATH="/usr/local/opt/ruby/bin:$PATH"

# GHCup
[ -f "/Users/wen/.ghcup/env" ] && source "/Users/wen/.ghcup/env" # ghcup-env

# Stack
export PATH="$HOME/.local/bin:$PATH"

# Cargo
export PATH="$HOME/.cargo/bin:$PATH"

# Opam
test -r /Users/wen/.opam/opam-init/init.sh && . /Users/wen/.opam/opam-init/init.sh > /dev/null 2> /dev/null || true

# Homebrew: coreutils, moreutils, findutils, gnu-sed
export PATH="$(brew --prefix coreutils)/libexec/gnubin:$PATH"
export PATH="$(brew --prefix moreutils)/libexec/gnubin:$PATH"
export PATH="$(brew --prefix findutils)/libexec/gnubin:$PATH"
export PATH="$(brew --prefix gnu-sed)/libexec/gnubin:$PATH"
export PATH="$(brew --prefix gawk)/libexec/gnubin:$PATH"

